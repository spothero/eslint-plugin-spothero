{
  "name": "@spothero/eslint-plugin-spothero",
  "version": "0.1.1",
  "description": "SpotHero custom ESLint plugin and rules",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "lib/index.js",
  "repository": "https://github.com/spothero/eslint-plugin-spothero",
  "scripts": {
    "test": "mocha tests --recursive",
    "release": "npm-release -- public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "eslint": "5.16.0",
    "requireindex": "~1.2.0"
  },
  "devDependencies": {
    "@spothero/commitlint-config": "2.1.0",
    "@spothero/npm-publisher": "2.3.0",
    "mocha": "6.1.4"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=6.1.0"
  },
  "license": "ISC"
}
